{
  "name": "cryptowallet",
  "version": "0.0.1",
  "description": "CryptoWallet - By Atlas City",
  "productName": "CryptoWallet",
  "cordovaId": "com.atlascity.wallet.mobile",
  "author": "James Kirkby <james.kirby@atlascity.io>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --fix --ext .js,.vue src",
    "test:unit": "cross-env NODE_ENV=test TZ=Europe/London jest",
    "test:e2e": "cross-env NODE_ENV=test wdio e2e.config.js",
    "test:unit:qa": "cross-env NODE_ENV=test jest --coverage",
    "test:unit:qa:codacy": "cross-env NODE_ENV=test jest --coverage && cat ./coverage/lcov.info | codacy-coverage",
    "test:unit:debug": "cross-env NODE_ENV=test jest --debug",
    "test:unit:watch": "cross-env NODE_ENV=test jest --watch",
    "test:unit:snapshot": "cross-env NODE_ENV=test jest --updateSnapshot",
    "server:instanbul": "cross-env NODE_ENV=test http-server ./tests/unit/__coverage__/lcov-report -p4200 -d -i -g -e -o --cors"
  },
  "dependencies": {
    "@atlascity/vuex-orm-plugin-lokijs": "^0.0.3",
    "@quasar/cli": "^1.0.0-beta.4",
    "@quasar/extras": "^1.1.0",
    "@vuex-orm/core": "^0.27.0",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "bip39": "^2.5.0",
    "bulma": "^0.7.1",
    "chart.js": "^2.7.3",
    "comlink": "^4.0.2",
    "cordova": "^8.1.2",
    "crypto": "^1.0.1",
    "crypto-js": "^3.1.9-1",
    "cryptowallet-js": "git://github.com/atlascity/cryptowallet-js.git",
    "etherscan-api": "^10.0.0",
    "module": "^1.2.5",
    "numeral": "^2.0.6",
    "qrcode": "^1.3.2",
    "quasar": "^1.0.0-beta.14",
    "snyk": "^1.130.0",
    "vue": "^2.6.7",
    "vue-axios": "^2.1.4",
    "vue-chartjs": "^3.4.0",
    "vue-form-wizard": "^0.8.4",
    "vue-i18n": "8.9.0",
    "vue-keyboard": "git://github.com/atlascity/vue-keyboard.git",
    "vue-rollbar": "^0.2.4",
    "vue-router": "^3.0.1",
    "vue-select": "^2.5.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.7",
    "vuelidate": "^0.7.4",
